{
	"name": "intelligent-web-news-agent",
	"version": "0.0.0",
	"description": "Angular application scaffold for the Intelligent Web News Agent.",
	"private": true,
	"engines": {
		"node": ">=20.17.0",
		"npm": ">=10.8.2"
	},
	"sideEffects": [
		"*.css",
		"*.scss"
	],
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:dev": "ng serve --configuration=development",
		"start:prod": "ng serve --configuration=production",
		"build": "ng build",
		"build:prod": "ng build --configuration=production",
		"build:dev": "ng build --configuration=development",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:ci": "jest --ci --coverage --maxWorkers=2",
		"test:coverage": "jest --coverage",
		"test:clear-cache": "jest --clearCache",
		"lint": "eslint \"src/**/*.{ts,html}\" --max-warnings=0",
		"lint:fix": "eslint \"src/**/*.{ts,html}\" --fix",
		"format": "prettier --write \"src/**/*.{ts,js,json,css,scss,html}\"",
		"format:check": "prettier --check \"src/**/*.{ts,js,json,css,scss,html}\"",
		"prepare": "husky install"
	},
	"dependencies": {
		"@angular/animations": "^18.2.0",
		"@angular/cdk": "^18.2.14",
		"@angular/common": "^18.2.0",
		"@angular/compiler": "^18.2.0",
		"@angular/core": "^18.2.0",
		"@angular/forms": "^18.2.0",
		"@angular/material": "^18.2.14",
		"@angular/platform-browser": "^18.2.0",
		"@angular/platform-browser-dynamic": "^18.2.0",
		"@angular/router": "^18.2.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.2.6",
		"@angular-eslint/eslint-plugin": "^18.0.0",
		"@angular-eslint/eslint-plugin-template": "^18.0.0",
		"@angular-eslint/template-parser": "^18.0.0",
		"@angular/cli": "^18.2.6",
		"@angular/compiler-cli": "^18.2.0",
		"@jest/create-cache-key-function": "^29.7.0",
		"@nx/jest": "^17.3.2",
		"@types/jest": "^29.5.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.2",
		"eslint-import-resolver-typescript": "^3.10.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jest": "^26.9.0",
		"eslint-plugin-jsdoc": "^35.5.1",
		"eslint-plugin-rxjs": "^5.0.3",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^7.0.4",
		"jest": "^29.5.0",
		"jest-canvas-mock": "^2.4.0",
		"jest-environment-jsdom": "^29.4.1",
		"jest-extended": "^2.0.0",
		"jest-junit": "^12.2.0",
		"jest-preset-angular": "^14.2.2",
		"jest-raw-loader": "^1.0.1",
		"lint-staged": "^11.0.1",
		"ng-mocks": "^14.14.0",
		"prettier": "^3.1.0",
		"ts-jest": "^29.1.0",
		"typescript": "~5.5.2"
	}
}
